QwtPlot {
    border-radius: 2px;
    background: #334;
}

QwtPlotCanvas {
    border-radius: 2px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 rgba(32, 32, 32, 255), stop: 1 rgba(32, 32, 32, 255));
}

QwtPlotZoomer {
    border-radius: 2px;
}

QwtTextLabel {
    color: white;
}

QwtScaleWidget {
    color: #bbb;
}

QwtLegendLabel {
    color:  white;
}

/* StatusBarMgr ModelMgrWidget */

ModelMgrWidget QLabel#labelName:hover {
    color: #fffe2f;
}

/* JCavansOptionWidget */

JCavansOptionWidget#JCavansOptionWidget {
    background: blue;
    border: 1px solid #888;
}
