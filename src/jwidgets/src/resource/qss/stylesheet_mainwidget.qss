
/* StatusBarMgr */
  
StatusBarMgr {
    background: #0e3e49;
}

StatusBarMgr QLabel#labelTime {
    color: #e0e0e0;
}
 
StatusBarMgr QLabel#labelInfo {
    color: white; 
    margin-left: 6px;
    margin-right: 6px;
}  

/** QwtPlot **/

QwtPlot {
    border-radius: 3px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 rgba(25, 25, 25, 255), stop: 1 rgba(25, 25, 25, 255));
}
